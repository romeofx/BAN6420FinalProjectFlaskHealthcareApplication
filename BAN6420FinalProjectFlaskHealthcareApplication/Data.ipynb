    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "# Connect to the SQLite database file\n",
    "conn = sqlite3.connect(\"survey.db\")\n",
    "\n",
    "# Load data from the participants table into a DataFrame\n",
    "df = pd.read_sql_query(\"SELECT * FROM participant\", conn)\n",
    "\n",
    "# Close the connection\n",
    "conn.close()\n",
    "\n",
    "# Display data\n",
    "df\n"
   ]
